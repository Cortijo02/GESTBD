@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sch: <https://schema.org/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix dat: <http://example.org/universidadesMadrid#> .

#######################################################################
# CLASES
#######################################################################

sch:EducationalOccupationalProgram a owl:Class .
sch:CourseInstance a owl:Class .
sch:School a owl:Class ;
    rdfs:subClassOf sch:Place .
sch:CollegeOrUniversity a owl:Class ;
    rdfs:subClassOf sch:Place .
sch:CategoryCode a owl:Class .
sch:CategoryCodeSet a owl:Class .
dat:CutOffScore a owl:Class .

#######################################################################
# RELACIONES ENTRE CLASES (OBJECT PROPERTIES)
#######################################################################

sch:CourseInstance rdfs:subClassOf [
    a owl:Restriction ;
    owl:onProperty sch:about ;
    owl:someValuesFrom sch:EducationalOccupationalProgram
] .

sch:CourseInstance rdfs:subClassOf [
    a owl:Restriction ;
    owl:onProperty sch:location ;
    owl:someValuesFrom sch:School
] .

sch:School rdfs:subClassOf [
    a owl:Restriction ;
    owl:onProperty sch:containedInPlace ;
    owl:someValuesFrom sch:CollegeOrUniversity
] .

sch:CollegeOrUniversity rdfs:subClassOf [
    a owl:Restriction ;
    owl:onProperty sch:containsPlace ;
    owl:someValuesFrom sch:School
] .

sch:CategoryCode rdfs:subClassOf [
    a owl:Restriction ;
    owl:onProperty sch:inCodeSet ;
    owl:someValuesFrom sch:CategoryCodeSet
] .

sch:EducationalOccupationalProgram rdfs:subClassOf [
    a owl:Restriction ;
    owl:onProperty sch:occupationalCategory ;
    owl:someValuesFrom sch:CategoryCode
] .

dat:CutOffScore rdfs:subClassOf [
    a owl:Restriction ;
    owl:onProperty dat:educational_program ;
    owl:someValuesFrom sch:CourseInstance
] .

#######################################################################
# ATRIBUTOS DE DATOS (DATA PROPERTIES)
#######################################################################

# EducationalOccupationalProgram
sch:name a owl:DatatypeProperty ;
    rdfs:domain sch:EducationalOccupationalProgram ;
    rdfs:range xsd:string .

sch:description a owl:DatatypeProperty ;
    rdfs:domain sch:EducationalOccupationalProgram ;
    rdfs:range xsd:string .

sch:occupationalCredentialAwarded a owl:DatatypeProperty ;
    rdfs:domain sch:EducationalOccupationalProgram ;
    rdfs:range xsd:string .

# School
sch:legalName a owl:DatatypeProperty ;
    rdfs:domain sch:School ;
    rdfs:range xsd:string .

sch:telephone a owl:DatatypeProperty ;
    rdfs:domain sch:School ;
    rdfs:range xsd:string .

sch:email a owl:DatatypeProperty ;
    rdfs:domain sch:School ;
    rdfs:range xsd:string .

sch:url a owl:DatatypeProperty ;
    rdfs:domain sch:School ;
    rdfs:range xsd:anyURI .

sch:address a owl:DatatypeProperty ;
    rdfs:domain sch:School ;
    rdfs:range xsd:string .

sch:postalCode a owl:DatatypeProperty ;
    rdfs:domain sch:School ;
    rdfs:range xsd:string .

# CollegeOrUniversity
sch:legalName a owl:DatatypeProperty ;
    rdfs:domain sch:CollegeOrUniversity ;
    rdfs:range xsd:string .

sch:additionalType a owl:DatatypeProperty ;
    rdfs:domain sch:CollegeOrUniversity ;
    rdfs:range xsd:string .

# CategoryCodeSet
sch:hasCategoryCode a owl:ObjectProperty ;
    rdfs:domain sch:CategoryCodeSet ;
    rdfs:range sch:CategoryCode .

# CutOffScore (nota de corte)
dat:value a owl:DatatypeProperty ;
    rdfs:domain dat:CutOffScore ;
    rdfs:range xsd:float .

dat:year a owl:DatatypeProperty ;
    rdfs:domain dat:CutOffScore ;
    rdfs:range xsd:gYear .

dat:call a owl:DatatypeProperty ;
    rdfs:domain dat:CutOffScore ;
    rdfs:range xsd:string .

dat:group a owl:DatatypeProperty ;
    rdfs:domain dat:CutOffScore ;
    rdfs:range xsd:string .
