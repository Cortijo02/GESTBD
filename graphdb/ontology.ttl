@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sch: <https://schema.org/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix dat: <http://example.org/universidadesMadrid#> .


<http://example.org/universidadesMadrid#> rdf:type owl:Ontology ;
    rdfs:label "Ontologia practica GESTDB" ;
    owl:versionInfo 1.0 .

#######################################################################
# CLASES
#######################################################################

sch:EducationalOccupationalProgram a owl:Class .

sch:CourseInstance a owl:Class .

sch:School a owl:Class ;
    rdfs:subClassOf sch:Place .

sch:CollegeOrUniversity a owl:Class ;
    rdfs:subClassOf sch:Place .

sch:CategoryCode a owl:Class .

sch:CategoryCodeSet a owl:Class .

dat:CutOffScore a owl:Class .

#######################################################################
# RELACIONES ENTRE CLASES (OBJECT PROPERTIES) -> no se si queda incluir las inversas (owl:inverseOf dat:hasOccupationalProgram .)
#######################################################################

sch:about a owl:ObjectProperty ;
    rdfs:domain sch:CourseInstance ;
    rdfs:range sch:EducationalOccupationalProgram .

sch:location a owl:ObjectProperty ;
    rdfs:domain sch:CourseInstance ;
    rdfs:range sch:School .

dat:educational_program a owl:ObjectProperty ;
    rdfs:domain dat:CutOffScore ;
    rdfs:range sch:CourseInstance .

sch:inCodeSet a owl:ObjectProperty ;
    rdfs:domain sch:CategoryCode ;
    rdfs:range sch:CategoryCodeSet .

sch:hasCategoryCode a owl:ObjectProperty ;
    rdfs:domain sch:CategoryCodeSet ;
    rdfs:range sch:CategoryCode .

sch:occupationalCategory a owl:ObjectProperty ;
    rdfs:domain sch:EducationalOccupationalProgram ;
    rdfs:range sch:CategoryCode .

sch:containedInPlace a owl:ObjectProperty ;
    rdfs:domain sch:School ;
    rdfs:range sch:CollegeOrUniversity .

sch:containsPlace a owl:ObjectProperty ;
    rdfs:domain sch:CollegeOrUniversity ;
    rdfs:range sch:School .

dat:hasOccupationalProgram a owl:ObjectProperty ;
    rdfs:domain sch:CategoryCode ;
    rdfs:range sch:EducationalOccupationalProgram .


#######################################################################
# ATRIBUTOS DE DATOS (DATA PROPERTIES)
#######################################################################

# EducationalOccupationalProgram
sch:name a owl:DatatypeProperty ;
    rdfs:domain sch:EducationalOccupationalProgram ;
    rdfs:range xsd:string .

sch:description a owl:DatatypeProperty ;
    rdfs:domain sch:EducationalOccupationalProgram ;
    rdfs:range xsd:string .

sch:occupationalCredentialAwarded a owl:DatatypeProperty ;
    rdfs:domain sch:EducationalOccupationalProgram ;
    rdfs:range xsd:string .

# CategoryCode | Puede ser name o codeValue
sch:name a owl:DatatypeProperty ;
    rdfs:domain sch:CategoryCode ;
    rdfs:range xsd:string .

# CategoryCodeSet
sch:name a owl:DatatypeProperty ;
    rdfs:domain sch:CategoryCodeSet ;
    rdfs:range xsd:string .

# CutOffScore
dat:value a owl:DatatypeProperty ;
    rdfs:domain dat:CutOffScore ;
    rdfs:range xsd:float .

dat:year a owl:DatatypeProperty ;
    rdfs:domain dat:CutOffScore ;
    rdfs:range xsd:gYear .

dat:call a owl:DatatypeProperty ;
    rdfs:domain dat:CutOffScore ;
    rdfs:range xsd:string .

dat:group a owl:DatatypeProperty ;
    rdfs:domain dat:CutOffScore ;
    rdfs:range xsd:string .

# School
sch:legalName a owl:DatatypeProperty ;
    rdfs:domain sch:School ;
    rdfs:range xsd:string .

sch:telephone a owl:DatatypeProperty ;
    rdfs:domain sch:School ;
    rdfs:range xsd:string .

sch:email a owl:DatatypeProperty ;
    rdfs:domain sch:School ;
    rdfs:range xsd:string .

sch:url a owl:DatatypeProperty ;
    rdfs:domain sch:School ;
    rdfs:range xsd:anyURI .

sch:address a owl:DatatypeProperty ;
    rdfs:domain sch:School ;
    rdfs:range xsd:string .

# CollegeOrUniversity
sch:legalName a owl:DatatypeProperty ;
    rdfs:domain sch:CollegeOrUniversity ;
    rdfs:range xsd:string .

sch:additionalType a owl:DatatypeProperty ;
    rdfs:domain sch:CollegeOrUniversity ;
    rdfs:range xsd:string .
